<footer>
    <div class="container">
            <div class="copy">
                <p>
                    &copy; 2019<script>new Date().getFullYear()>2010&&document.write("-"+new Date().getFullYear());</script>, Content By <%- config.author %>. All Rights Reserved.
                </p>
                <!--<p>Theme By <a href="//go.kieran.top" style="color: #767D84">Kieran</a></p-->
            </div>
            <div class="social">
                <ul>
                    <% if(theme.github){ %>
                    <li><a href="<%- theme.github %>" title="Github" target="_blank"><i class="fab fa-github"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.twitter){ %>
                    <li><a href="<%- theme.twitter %>" title="Twitter" target="_blank"><i class="fab fa-twitter"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.linkedin){ %>
                    <li><a href="<%- theme.linkedin %>" title="Linkedin" target="_blank"><i class="fab fa-linkedin"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.instagram){ %>
                    <li><a href="<%- theme.instagram %>" title="Instagram" target="_blank"><i class="fab fa-instagram"></i></a>&nbsp;</li>
                    <% } %>
                    <% if(theme.medium){ %>
                    <li><a href="<%- theme.medium %>" title="Medium" target="_blank"><i class="fab fa-medium"></i></a></li>
                    <% } %>
                </ul>
            </div>
            <div class="clearfix"> </div>
        </div>
</footer>

